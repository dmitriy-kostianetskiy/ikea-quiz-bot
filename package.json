{
  "name": "ikea-quiz-bot",
  "version": "3.0.0",
  "description": "Simple quiz-bot for Telegram",
  "main": "index.js",
  "scripts": {
    "start": "ts-node index.ts",
    "build": "tsc --project tsconfig.json",
    "lint": "tslint --project tsconfig.json",
    "encrypt:dev": "gcloud kms encrypt --plaintext-file=environments/DEV.yaml --ciphertext-file=environments/DEV.yaml.enc --location=global --keyring=ikea-quiz-bot-key-ring --key=ikea-quiz-bot-key",
    "encrypt:prod": "gcloud kms encrypt --plaintext-file=environments/PROD.yaml --ciphertext-file=environments/PROD.yaml.enc --location=global --keyring=ikea-quiz-bot-key-ring --key=ikea-quiz-bot-key"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redrick-tmn/ikea-quiz-bot.git"
  },
  "author": "Dmitry Kostianetsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redrick-tmn/ikea-quiz-bot/issues"
  },
  "homepage": "https://github.com/redrick-tmn/ikea-quiz-bot#readme",
  "dependencies": {
    "telegraf": "^3.36.0",
    "ts-loader": "^6.2.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.9.1",
    "typescript": "^3.7.5"
  }
}
